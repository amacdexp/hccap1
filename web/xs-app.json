{
  "welcomeFile": "index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "(catalog/)(.*)",
      "destination": "hccap1_srv_be",
      "csrfProtection": true,
      "authenticationType": "xsuaa"
    },
    {
      "source": "(services/)(.*)",
      "destination": "hccap1_srv_be",
      "csrfProtection": true,
      "authenticationType": "xsuaa"
    },
    {
      "localDir": "resources",
      "source": "^/(.*)",
      "authenticationType": "none"
    }
  ]
}

